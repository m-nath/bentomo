<h3> Kitchens</h3>

<% @kitchens.each do |kitchen|%>
  <%= cl_image_tag kitchen.photo %>
  <%= link_to kitchen.name, kitchen_path(kitchen) %>
  <%= kitchen.description%>
<% end %>

<p><%= link_to "more kitchens", kitchens_path %></p>




<h3> Plans </h3>
<% @plans.each do |plan|%>
  <%= cl_image_tag plan.photo %>
  <%= link_to plan.name, plan_path(plan) %>
  <%= plan.price%>
<% end %>

<p><%= link_to "more plans", plans_path %></p>

